{
    "name": "interlutions/coding-standard",
    "type": "phpcodesniffer-standard",
    "description": "Internal PHP coding standards of Interlutions",
    "keywords": ["coding standard", "phpcs", "psr", "code sniffer", "symfony", "neos", "typo3"],
    "homepage": "https://www.interlutions.de/",
    "license": "MIT",
    "autoload-dev": {
        "psr-4": {
            "Interlutions\\Sniffs\\": "Interlutions/Sniffs/"
        }
    },
    "authors": [
        {
            "name": "Interlutions Team",
            "homepage": "https://www.interlutions.de/agentur"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^4.0 || ^5.0 || ^6.0"
    },
    "conflict": {
        "squizlabs/php_codesniffer": "<3 || >=4"
    }
}
